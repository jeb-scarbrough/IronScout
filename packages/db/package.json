{
  "name": "@ironscout/db",
  "version": "1.0.0",
  "description": "Database schema and utilities for IronScout.ai",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./test-utils": {
      "types": "./test-utils.d.ts",
      "import": "./test-utils.js",
      "default": "./test-utils.js"
    },
    "./generated/prisma": {
      "types": "./generated/prisma/index.d.ts",
      "import": "./generated/prisma/index.js",
      "default": "./generated/prisma/index.js"
    },
    "./visibility.js": {
      "types": "./visibility.d.ts",
      "import": "./visibility.js",
      "default": "./visibility.js"
    }
  },
  "scripts": {
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:baseline": "tsx scripts/baseline-migrations.ts",
    "db:studio": "prisma studio",
    "db:seed": "tsx seed.ts",
    "db:seed-source": "tsx seed-source.ts",
    "db:seed-ammo": "tsx seed-ammo-data.ts",
    "db:seed-retailers": "tsx seed-retailers.ts",
    "db:seed-price-history": "tsx seed-price-history.ts",
    "db:seed-comprehensive": "tsx seed-comprehensive-products-simple.ts",
    "db:clear": "tsx clear-seed-data.ts",
    "check": "tsx scripts/prestart-check.ts",
    "check:fast": "tsx scripts/prestart-check.ts",
    "check:strict": "tsx scripts/prestart-check.ts --strict",
    "check:drift": "tsx scripts/check-schema-drift.ts",
    "audit:migrations": "tsx scripts/audit-migrations.ts",
    "audit:migrations:verbose": "tsx scripts/audit-migrations.ts --verbose",
    "audit:db": "tsx scripts/db-audit.ts",
    "check:client": "tsx scripts/check-client-fresh.ts",
    "check:client:strict": "tsx scripts/check-client-fresh.ts strict",
    "validate:schema": "tsx scripts/runtime-schema-check.ts",
    "test:schema": "vitest run __tests__/schema-integrity.test.ts",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/client-runtime-utils": "^7.3.0",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.11.0",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.4",
    "prisma": "^7.3.0",
    "testcontainers": "^11.11.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  }
}
